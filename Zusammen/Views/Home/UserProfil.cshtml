@model Zusammen.Models.users
@{
    ViewBag.Title = "Обліковий запис";
    ViewData["ShowHeader"] = true;
}
<script src="~/js/new_photo.js"></script> 
<div class="profile" id="profileUs" >
    <div class="photo" id="uploadedImage1">
        <img src="../img/photo.ico">
    </div>
    <div class="dataUs">
        <p class="data">Нікнейм:<span>@Model.nickname</span> </p>
        <p class="data">Про себе:<span>@Model.profile_description</span> </p>
        <p class="data">Кімнати: <span>@Model.rooms</span></p>
    </div>
</div>
@using (Html.BeginForm("ChangeUserProfil", "Home", FormMethod.Post))
{
    <div id="editButtonContainer">
        <button class="buttonroom2" type="submit">Редагувати</button>
    </div>
}